{
  "version": 2,
  "builds": [
    {
      "src": "frontend_book/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd frontend_book && npm install && npm run build",
        "outputDirectory": "frontend_book/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend_book/build/$1",
      "status": 200
    }
  ]
}